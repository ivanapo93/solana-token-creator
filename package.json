{
  "name": "solmeme-creator-backend",
  "version": "1.0.0",
  "description": "Production backend for SolMeme Creator with real token minting and AI generation",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "build": "npm install",
    "test": "jest",
    "test-spl-token": "node test-node-imports.js",
    "verify-imports": "node verify-modern-imports.js",
    "test-createMint": "node test-modern-createMint.js",
    "install-spl-deps": "bash install-dependencies.sh",
    "validate-setup": "node setup-validator.js",
    "setup": "npm install && node setup-validator.js",
    "db-setup": "node -e \"console.log('Please run the SQL commands from BACKEND_SETUP_GUIDE.md in your Supabase dashboard')\"",
    "health-check": "curl -f http://localhost:3000/api/promo/health || exit 1"
  },
  "keywords": [
    "solana",
    "token",
    "minting",
    "blockchain",
    "web3",
    "metaplex"
  ],
  "author": "SolMeme Creator",
  "license": "MIT",
