    "arweave": "^1.14.4",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bs58": "^1.0.0",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
