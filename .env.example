# ===========================================
# SOLANA TOKEN CREATOR - ENVIRONMENT VARIABLES
# ===========================================
# Copy this file to .env and fill in your actual API keys
# NEVER commit your actual .env file to version control

# ===========================================
# IPFS SERVICES CONFIGURATION
# ===========================================

# PRIMARY IPFS SERVICE - PINATA
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here

# FALLBACK IPFS SERVICE #1 - NFT.STORAGE
NFT_STORAGE_TOKEN=your_nft_storage_api_key_here

# FALLBACK IPFS SERVICE #2 - WEB3.STORAGE
WEB3_STORAGE_TOKEN=your_web3_storage_api_key_here

# OPTIONAL FALLBACK - STORACHA
STORACHA_DID_KEY=your_storacha_did_key_here

# ===========================================
# SOLANA NETWORK CONFIGURATION
# ===========================================

# Solana RPC URL (using Alchemy for production)
SOLANA_RPC_URL=https://solana-mainnet.g.alchemy.com/v2/PVF7BjfV8uajJQcxoAN0D

# Helius API (if using Helius integration)
HELIUS_API_KEY=your_helius_api_key_here

# ===========================================
# DEXSCREENER INTEGRATION
# ===========================================

# Dexscreener API configuration
DEXSCREENER_API_URL=https://api.dexscreener.com
DEXSCREENER_SUBMIT_TOKEN_ENDPOINT=/latest/dex/tokens

# ===========================================
# BACKEND SERVICES (if using backend)
# ===========================================

# Supabase configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# JWT Secret for authentication
JWT_SECRET=your_jwt_secret_here

# Telegram notifications (optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# ===========================================
# SECURITY SETTINGS
# ===========================================

# Environment mode
NODE_ENV=development

# CORS allowed origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Rate limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

